<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1>投稿画面</h1>
      <%= form_for @topic do |f| %>
        <%= f.label :cafe_name  %>
        <%= f.text_field :cafe_name,class: 'form-control'  %>

        <%= f.label :content  %>
        <%= f.text_field :content,class: 'form-control'  %>

        <%= f.file_field :picture %>
        
        <%= f.submit '投稿',class: "btn btn-primary" %>
      <% end %>
    </div>
</div>
